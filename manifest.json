{
  "name": "Youtube Highler",
  "description": "Early concept WIP",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "css": ["styles.css"],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
